{
    "name": "josh-cunningham/php-auth0-test",
    "description": "Testing the Auth0 PHP SDK",
    "type": "project",
    "require": {
        "php": "^5.5 || ^7.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Auth0\\SDK\\Scaffold\\": "classes",
            "Auth0\\SDK\\": "auth0/src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Cunningham",
            "email": "josh@joshcanhelp.com"
        }
    ],
    "scripts": {
        "php-compat" : "phpcs ./auth0 --standard=PHPCompatibility --runtime-set testVersion 5.5 --extensions=php --ignore=examples/*,tests/*,vendor/bin/*,vendor/composer/*,vendor/doctrine/*,vendor/josegonzalez/*,vendor/m1/*,vendor/myclabs/*,vendor/phpdocumentor/*,vendor/phpspec/*,vendor/phpunit/*,vendor/sebastian/*,vendor/symfony/*,vendor/webmozart/*"
    }
}
